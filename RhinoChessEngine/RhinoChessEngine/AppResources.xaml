<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" 
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:RhinoChessEngine">
    <ResourceDictionary.MergedDictionaries>
        <ResourceDictionary Source="Resources/ChessPieceResources.xaml"></ResourceDictionary>
    </ResourceDictionary.MergedDictionaries>
    <SolidColorBrush x:Key="LightSquareColour" Color="#f0d9b5" />
    <SolidColorBrush x:Key="DarkSquareColour" Color="#B58863" />
    <SolidColorBrush x:Key="DarkBackground" Color="#1f1f1f" />
    <FontFamily x:Key="SmallFont">Noda Sans</FontFamily>
</ResourceDictionary>
